    <style>
        .nav-link {
            background-image: linear-gradient( to right, #4154f1, #4154f1 50%, #000 25% );
            background-size: 200% 100%;
            background-position: -100%;
            display: inline-bl ock;
            padding: 5px 0;
            position: relative;
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
            transition: all 0.3s ease-in-out;
        }

            .nav-link:before {
                content: '';
                background: #4154f1;
                display: block;
                position: absolute;
                bottom: -3px;
                left: 0;
                width: 0;
                height: 2px;
                transition: all 0.3s ease-in-out;
            }

            .nav-link:hover {
                background-position: 0;
            }

                .nav-link:hover::before {
                    width: 100%;
                }
    </style>
